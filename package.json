{
  "name": "template-node-trpc-react",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jfortez/ntrvd"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "services:up": "docker-compose --env-file ./apps/server/.env -f docker-compose-dev.yml up -d"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
